@section Scripts {
    <script src="~/wwroot/mine/medicalRecordEdit.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            setValidationRuleForPatient();
            setValidationRuleForDoctorChecking();
            setValidationRuleForMedication();
            setValidationRuleForMonitoringProgress();

            @if(Model.DoctorCheckings != null)
            {
                for(int i = 0; i < Model.DoctorCheckings.Count(); i++)
                {
                    @: var value = moment('@Model.DoctorCheckings[i].CheckedDateTime', "YYYYMMDDHHmmss");
                    @: value = value.format("YYYY-MM-DD HH:mm:ss");
                    @: var item = {
                        @: "CheckedDateTime": decodeData(value),
                        @: "DoctorName": decodeData('@Model.DoctorCheckings[i].DoctorName'),
                        @: "Remarks": decodeData('@Model.DoctorCheckings[i].Remarks'),
                    @: };

                    @: dataDoctorChecking.push(item);
                }
            }

            @if(Model.MedicationPlans != null)
            {
                for(int i = 0; i < Model.MedicationPlans.Count(); i++)
                {
                    @: var value = moment('@Model.MedicationPlans[i].MedicatedDate', "YYYYMMDDHHmmss");
                    @: value = value.format("YYYY-MM-DD HH:mm:ss");
                    @: var item = {
                        @: "MedicatedDate": decodeData(value),
                        @: "Drug": decodeData('@Model.MedicationPlans[i].DrugId'),
                        @: "Dose": decodeData('@Model.MedicationPlans[i].Dose'),
                        @: "Route": decodeData('@Model.MedicationPlans[i].Route'),
                        @: "Remarks": decodeData('@Model.MedicationPlans[i].Remarks'),
                    @: };

                    @: dataMedicationPlan.push(item);
                }
            }

            @if(Model.MonitoringProgresses != null)
            {
                for(int i = 0; i < Model.MonitoringProgresses.Count(); i++)
                {
                    @: var value = moment('@Model.MonitoringProgresses[i].MonitoredDateTime', "YYYYMMDDHHmmss");
                    @: value = value.format("YYYY-MM-DD HH:mm:ss");
                    @: var item = {
                        @: "MonitoredDateTime": decodeData(value),
                        @: "BP": decodeData('@Model.MonitoringProgresses[i].BP'),
                        @: "PR": decodeData('@Model.MonitoringProgresses[i].PR'),
                        @: "RR": decodeData('@Model.MonitoringProgresses[i].RR'),
                        @: "SpO2": decodeData('@Model.MonitoringProgresses[i].SpO2'),
                        @: "GCS": decodeData('@Model.MonitoringProgresses[i].GCS'),
                        @: "FluidTotal": decodeData('@Model.MonitoringProgresses[i].FluidTotal'),
                        @: "UrineOutput": decodeData('@Model.MonitoringProgresses[i].UrineOutput'),
                        @: "Antibiotic": decodeData('@Model.MonitoringProgresses[i].Antibiotic'),
                        @: "PainKiller": decodeData('@Model.MonitoringProgresses[i].PainKiller'),
                        @: "Remarks": decodeData('@Model.MonitoringProgresses[i].Remarks'),
                    @: };

                    @: dataMonitoringProgress.push(item);
                }
            }

            @if(Model.MedicalRecordItems != null)
            {
                for(int i = 0; i < Model.MedicalRecordItems.Count(); i++)
                {
                    @: var item = {
                        @: "Code": decodeData('@Model.MedicalRecordItems[i].Code'),
                        @: "Value": decodeData('@Model.MedicalRecordItems[i].Value.Replace("\n", "<br>")'),
                        @: "CodeType": decodeData('@Model.MedicalRecordItems[i].CodeType'),
                        @: "UIReference": decodeData('@Model.MedicalRecordItems[i].UIReference'),
                    @: };

                    @: dataMedicalRecordItems.push(item);
                }
            }

            tblDoctorsChecking = bindDoctorChecking();
            tblMedicationPlan = bindMedication();
            tblMonitoringProgress = bindMonitoringProgress();
            fillData();

            $(".select2").select2();

            $('#txtCheckedDateTime').datetimepicker({
                format: "YYYY-MM-DD HH:mm:ss",
                allowInputToggle: true,
                showTodayButton: true,
                showClose: true,
            });

            $('#txtMedicatedDate').datetimepicker({
                format: "YYYY-MM-DD",
                allowInputToggle: true,
                showTodayButton: true,
                showClose:true,
            });

            $('#txtMonitoredDateTime').datetimepicker({
                format: "YYYY-MM-DD HH:mm:ss",
                allowInputToggle: true,
                showTodayButton: true,
                showClose:true,
            });

            $('#txtRegistrationDate').datetimepicker({
                format: "YYYY-MM-DD",
                allowInputToggle: true,
                showClose:true,
                showTodayButton: true
            });

            $('#txtDOB').datetimepicker({
                format: "YYYY-MM-DD",
                allowInputToggle: true,
                showClose: true,
                showTodayButton: true
            });

            $('#txtArrivalTime').datetimepicker({
                format: "YYYY-MM-DD HH:mm:ss",
                allowInputToggle: true,
                showTodayButton: true,
                showClose: true,
            });

            $('#txtTreatmentTime').datetimepicker({
                format: "YYYY-MM-DD HH:mm:ss",
                allowInputToggle: true,
                showTodayButton: true,
                showClose: true,
            });

            $('#txtDCTime').datetimepicker({
                format: "YYYY-MM-DD HH:mm:ss",
                allowInputToggle: true,
                showTodayButton: true,
                showClose: true,
            });
        });
    </script>
}

